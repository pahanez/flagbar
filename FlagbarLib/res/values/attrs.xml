<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="flagbar">
        <attr name="stripesCount" format="integer">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
        </attr>

        <attr name="firstLineColor" format="color" />
        <attr name="secondLineColor" format="color" />
        <attr name="thirdLineColor" format="color" />
        <attr name="fourthLineColor" format="color" />

        <attr name="firstLineSpeed" format="integer">
            <enum name="high" value="7" />
            <enum name="medium" value="5" />
            <enum name="low" value="2" />
        </attr>
        <attr name="secondLineSpeed" format="integer">
            <enum name="high" value="7" />
            <enum name="medium" value="5" />
            <enum name="low" value="2" />
        </attr>
        <attr name="thirdLineSpeed" format="integer">
            <enum name="high" value="7" />
            <enum name="medium" value="5" />
            <enum name="low" value="2" />
        </attr>
        <attr name="fourthLineSpeed" format="integer">
            <enum name="high" value="7" />
            <enum name="medium" value="5" />
            <enum name="low" value="2" />
        </attr>


    </declare-styleable>
</resources>
